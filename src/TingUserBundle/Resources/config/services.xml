<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="ccmbenchmark_tinguserbundle.user_repository" class="AppBundle\User\UserRepository">
            <factory service="ting" method="get"></factory>
            <argument>AppBundle\User\UserRepository</argument>
        </service>
        <service id="ccmbenchmark_tinguserbundle.user_manager" class="CCMBenchmark\TingUserBundle\Manager\UserManager">
            <argument type="service" id="security.encoder_factory" />
            <argument type="service" id="fos_user.util.username_canonicalizer" />
            <argument type="service" id="fos_user.util.email_canonicalizer" />
            <call method="setUserRepository">
                <argument type="service" id="ccmbenchmark_tinguserbundle.user_repository" />
            </call>
        </service>
        <service id="ccmbenchmark_tinguserbundle.group_manager" class="CCMBenchmark\DamBundle\Twig\TwigExtension"></service>
    </services>
</container>
